{
  "manifest_version": 2,
  "name": "Youtube Members' Chat Filter",
  "version": "1.0.0",
  "description": "__MSG_ext_desc__",
  "default_locale": "en",
  "content_scripts": [{
    "matches": [
      "https://www.youtube.com/live_chat?*",
      "https://www.youtube.com/live_chat_replay?*"
    ],
    "all_frames": true,
    "js": ["content.js"],
    "css": ["content.css"]
  }]
}
